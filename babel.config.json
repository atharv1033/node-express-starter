{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "@": "./src",
                    "@core": "./src/@core",
                    "underscore": "lodash"
                }
            }
        ],
        [
            "@babel/transform-runtime"
        ]
    ]
}